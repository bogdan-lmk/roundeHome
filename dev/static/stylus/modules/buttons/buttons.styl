.btn
  display: flex;
  align-items: center;
  justify-content: center;
  text-align: center;
  border-radius: 25px;
  text-transform: uppercase;
  font-family: openSansSemi
  cursor: pointer;
.btn-header
  border: 2px solid #edab4d
  background: #47494f
  color: #edab4d
  font-size: 12px
  padding: 10px 15px
  z-index: 99

.btn-request-price
  background-color: #f0b151
  width: 100%
  padding: 15px 0
  margin-top: 30px
  font-size: 14px


.btn-help
  width: 60%
  font-family: openSansBold
